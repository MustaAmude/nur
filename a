        function updateSurahList() {
            const reciterSelect = document.getElementById('reciter');
            const selectedReciter = reciterSelect.value;
            const surahSelect = document.getElementById('surah');
            const reciterImage = document.getElementById('reciterImage');
            const reciterName = document.getElementById('reciterName');

            surahSelect.innerHTML = '<option value="" disabled selected>اختر السورة</option>';

            if (selectedReciter) {
                const reciter = reciters[selectedReciter];

                reciterName.textContent = reciter.name;
                reciterImage.src = reciter.image;

                for (const [surah, url] of Object.entries(reciter.surahs)) {
                    const option = document.createElement('option');
                    option.value = url;
                    option.textContent = surah;
                    surahSelect.appendChild(option);
                }
            }
        }

        function playSurah() {
            const surah = document.getElementById('surah').value;
            const audioPlayer = document.getElementById('audioPlayer');
            
            if (surah) {
                audioPlayer.src = surah;
                audioPlayer.load();
                audioPlayer.play();
            } else {
                alert('يرجى اختيار السورة.');
            }
        }
    </script>
</body>
</html>
